{
  "extends": "@istanbuljs/nyc-config-typescript",
  "all": true,
  "check-coverage": true,
  "reporter": [
    "text",
    "html",
    "lcov"
  ],
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "src/**/*.d.ts",
    "src/**/*.test.ts",
    "src/**/*.spec.ts",
    "test/**/*",
    "out/**/*",
    "dist/**/*",
    "node_modules/**/*"
  ],
  "extension": [
    ".ts"
  ],
  "cache": true,
  "temp-dir": ".nyc_output",
  "report-dir": "coverage",
  "lines": 80,
  "statements": 80,
  "functions": 80,
  "branches": 70
}